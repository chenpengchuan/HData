<?xml version="1.0" encoding="UTF-8"?>
<configuration status="off">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="[%-5p] %d{yyyy-MM-dd HH:mm:ss,SSS} method:%l%n%m%n" />
		</Console>
	</appenders>
	<loggers>
		<root level="info">
			<appender-ref ref="Console" />
		</root>
	</loggers>
</configuration>